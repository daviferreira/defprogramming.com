{
  "name": "defprogramming.com",
  "private": true,
  "description": "Quotes about coding",
  "version": "0.1.0",
  "author": "Davi Ferreira <hi@daviferreira.com>",
  "dependencies": {
    "gatsby": "^2.15.29",
    "gatsby-image": "^2.2.24",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-offline": "^3.0.11",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-sharp": "^2.2.28",
    "gatsby-source-filesystem": "^2.1.29",
    "gatsby-transformer-sharp": "^2.2.20",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard-react": "^9.2.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^3.1.1"
  },
  "keywords": [
    "gatsby",
    "coding",
    "quotes"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --port 4001",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint": "eslint . --cache --cache-location node_modules/.eslintcache && prettier --check '**/*.{js,css,json}' && stylelint './src/**/*.css'",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daviferreira/defprogramming.com"
  },
  "bugs": {
    "url": "https://github.com/daviferreira/defprogramming.com"
  },
  "engines": {
    "node": "12.11.1",
    "npm": "~6.9"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --check"
    ],
    "**/*.css": [
      "prettier --check",
      "stylelint"
    ],
    "**/*.json": [
      "prettier --check"
    ]
  }
}
